#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/Users/markstevens/dev/projects/bundle-constraint-plugin/node_modules/.pnpm/registry.npmjs.org/typescript/3.7.4/node_modules/typescript/bin/tsserver/node_modules:/Users/markstevens/dev/projects/bundle-constraint-plugin/node_modules/.pnpm/registry.npmjs.org/typescript/3.7.4/node_modules/typescript/bin/node_modules:/Users/markstevens/dev/projects/bundle-constraint-plugin/node_modules/.pnpm/registry.npmjs.org/typescript/3.7.4/node_modules/typescript/node_modules:/Users/markstevens/dev/projects/bundle-constraint-plugin/node_modules/.pnpm/registry.npmjs.org/typescript/3.7.4/node_modules:/Users/markstevens/dev/projects/bundle-constraint-plugin/node_modules/.pnpm/registry.npmjs.org/typescript/node_modules:/Users/markstevens/dev/projects/bundle-constraint-plugin/node_modules/.pnpm/registry.npmjs.org/node_modules:/Users/markstevens/dev/projects/bundle-constraint-plugin/node_modules/.pnpm/node_modules:/Users/markstevens/dev/projects/bundle-constraint-plugin/node_modules:/Users/markstevens/dev/projects/node_modules:/Users/markstevens/dev/node_modules:/Users/markstevens/node_modules:/Users/node_modules:/node_modules:/Users/markstevens/dev/projects/bundle-constraint-plugin/node_modules/typescript/bin/tsserver/node_modules:/Users/markstevens/dev/projects/bundle-constraint-plugin/node_modules/typescript/bin/node_modules:/Users/markstevens/dev/projects/bundle-constraint-plugin/node_modules/typescript/node_modules" "$basedir/node"  "$basedir/../typescript/bin/tsserver" "$@"
  ret=$?
else 
  NODE_PATH="/Users/markstevens/dev/projects/bundle-constraint-plugin/node_modules/.pnpm/registry.npmjs.org/typescript/3.7.4/node_modules/typescript/bin/tsserver/node_modules:/Users/markstevens/dev/projects/bundle-constraint-plugin/node_modules/.pnpm/registry.npmjs.org/typescript/3.7.4/node_modules/typescript/bin/node_modules:/Users/markstevens/dev/projects/bundle-constraint-plugin/node_modules/.pnpm/registry.npmjs.org/typescript/3.7.4/node_modules/typescript/node_modules:/Users/markstevens/dev/projects/bundle-constraint-plugin/node_modules/.pnpm/registry.npmjs.org/typescript/3.7.4/node_modules:/Users/markstevens/dev/projects/bundle-constraint-plugin/node_modules/.pnpm/registry.npmjs.org/typescript/node_modules:/Users/markstevens/dev/projects/bundle-constraint-plugin/node_modules/.pnpm/registry.npmjs.org/node_modules:/Users/markstevens/dev/projects/bundle-constraint-plugin/node_modules/.pnpm/node_modules:/Users/markstevens/dev/projects/bundle-constraint-plugin/node_modules:/Users/markstevens/dev/projects/node_modules:/Users/markstevens/dev/node_modules:/Users/markstevens/node_modules:/Users/node_modules:/node_modules:/Users/markstevens/dev/projects/bundle-constraint-plugin/node_modules/typescript/bin/tsserver/node_modules:/Users/markstevens/dev/projects/bundle-constraint-plugin/node_modules/typescript/bin/node_modules:/Users/markstevens/dev/projects/bundle-constraint-plugin/node_modules/typescript/node_modules" node  "$basedir/../typescript/bin/tsserver" "$@"
  ret=$?
fi
exit $ret
